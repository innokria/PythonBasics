high = False
income = False


if high and income:
    print('matrix')


elif income:
     print("world")


else:
    print('number')

--------------------------

guessCount = 0
chances = 3
secret = 9

while guessCount < chances:
    guessCount += 1
    if int(input('enter number ? ')) == secret:
        print("Win")
        break
    if guessCount == chances:
        print("All done")
        break
else:
    print('Keep guessing')
